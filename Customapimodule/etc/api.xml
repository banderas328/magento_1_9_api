<?xml version="1.0"?>
<config>
    <api>
        <resources>
            <customapimodule_api translate="title" module="customapimodule">
                <model>customapimodule/api_api</model>
                <title>Demo Custommoduleapi API</title>
                <acl>customapimodule/api</acl>
                <methods>
                    <list translate="title" module="customapimodule">
                        <title>List of data</title>
                        <method>items</method>
                    </list>
                </methods>
            </customapimodule_api>
        </resources>
        <resources_alias>
            <api>customapimodule_api</api>
        </resources_alias>
        <v2>
            <resources_function_prefix>
                <api>customapimoduleApi</api>
            </resources_function_prefix>
        </v2>
        <acl>
            <resources>
                <customapimodule translate="title" module="customapimodule">
                    <title>Apis</title>
                    <sort_order>5</sort_order>
                    <api translate="title" module="customapimodule">
                        <title>Api data</title>
                    </api>
                </customapimodule>
            </resources>
        </acl>
    </api>
</config>